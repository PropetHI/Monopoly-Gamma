<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="480.0" stylesheets="@css/popup-Style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adrian.roszkowski.monopolygamma.vidmot.TilePopupWindow">
   <children>
      <Label fx:id="TileName_ID" alignment="CENTER" layoutX="77.0" layoutY="14.0" prefHeight="110.0" prefWidth="327.0" text="Name" textAlignment="CENTER">
         <font>
            <Font name="Times New Roman" size="38.0" />
         </font>
      </Label>
      <Label fx:id="PricePerLevelText_ID" layoutX="77.0" layoutY="215.0" prefHeight="145.0" prefWidth="130.0" text="Label" />
      <Label fx:id="RentPerLevelText_ID" alignment="CENTER_RIGHT" layoutX="274.0" layoutY="215.0" prefHeight="145.0" prefWidth="130.0" text="Label" textAlignment="RIGHT" />
      <Button fx:id="BuyPropertyTileButton_ID" layoutX="77.0" layoutY="619.0" mnemonicParsing="false" onAction="#OnBuyProperty" prefHeight="58.0" prefWidth="121.0" text="Buy">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="SellButton_ID" layoutX="283.0" layoutY="619.0" mnemonicParsing="false" onAction="#OnSellProperty" prefHeight="58.0" prefWidth="121.0" text="Sell">
         <font>
            <Font name="Cambria" size="20.0" />
         </font></Button>
      <ImageView fitHeight="200.0" fitWidth="200.0" layoutX="140.0" layoutY="285.0" pickOnBounds="true" preserveRatio="true" styleClass="houseImage" />
   </children>
</AnchorPane>
